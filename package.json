{
  "name": "recommender-feedback",
  "description": "Recommender Feedback web server",
  "version": "0.1.0",
  "homepage": "https://github.com/jpsecher/node-service#readme",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/recommender-feedback.git"
  },
  "author": "DBCDK",
  "bugs": {
    "url": "https://github.com/DBCDK/recommender-feedback/issues"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.3.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nsrun": "^1.1.2",
    "react-scripts": "1.0.7",
    "sinon": "^3.1.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "knex": "^0.13.0",
    "pg": "^7.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "proxy": "http://localhost:3001",
  "scripts": {
    "build": "react-scripts build",
    "delayed-integration-test": "sleep 3 && nsrun integration-test && ./kill-running-server.sh",
    "full-test": "run-p serve-once delayed-integration-test",
    "integration-test": ". ./current.env && mocha src/integration/**/*_test.js",
    "lint": "eslint --format unix src",
    "postinstall": "./setup-node-env.sh",
    "serve": ". ./current.env && nodemon --exec node --no-stdin src/server/main.js --watch src/server",
    "serve-once": ". ./current.env && node src/server/main.js || true",
    "start": "run-p serve start-frontend",
    "start-frontend": "react-scripts start",
    "test": "run-s lint unit-test",
    "unit-test": "mocha src/lib/**/*_test.js"
  }
}
